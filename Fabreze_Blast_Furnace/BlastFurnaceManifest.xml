<manifest>
    <main-class>Fabreze.bots.Fabreze_Blast_Furnace.BlastFurnaceMain</main-class>
    <name>Fabreze Blast Furnace</name>
    <tag-line> Who knew Blast Furnace would be such a breeze </tag-line><!--Max of 50 chars-->
    <description>Plays Blast Furnace for Smithing xp</description><!--Max of 110 chars-->
    <version>1.1.2</version>
    <compatibility>
        <game>OSRS</game>
    </compatibility>
    <categories>
        <category>SMITHING</category>
    </categories>
    <!--Required to publish on the bot store-->
    <internal-id>Blast Furnace</internal-id>
    <!--The rest are optional-->
    <open-source>false</open-source>
    <hidden>false</hidden> <!--If you need to hide it from the bot store for maintenance-->
    <access>public</access>
    <tags>
        <tag>Blast Furnace</tag>
        <tag>Smithing</tag>
        <tag>Mithril</tag>
        <tag>Ice gloves</tag>
    </tags>
    <resources>
        <resource>Fabreze/bots/Fabreze_Blast_Furnace/GUI/Blast_FurnaceGUI.fxml</resource>
    </resources>
</manifest>